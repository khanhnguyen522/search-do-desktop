{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "search-do",
  "version": "0.1.0",
  "identifier": "com.khanhnguyen.search-do",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "macOSPrivateApi": true,
    "windows": [
      {
        "label": "main",
        "title": "search-do",
        "width": 720,
        "height": 900,
        "alwaysOnTop": true,
        "visibleOnAllWorkspaces": true,
        "resizable": false,
        "fullscreen": false,
        "transparent": true,
        "decorations": false,
        "shadow": true,
        "visible": false
      }
    ],
    "security": { "csp": null }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
